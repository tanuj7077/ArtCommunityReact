{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "@material-ui/core": "^4.11.3",
    "@popperjs/core": "^2.9.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "autoprefixer": "^7.2.6",
    "axios": "^0.21.1",
    "browser-image-compression": "^1.0.17",
    "firebase": "^8.2.10",
    "flickity": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.2",
    "react": "^17.0.1",
    "react-avatar-editor": "^11.1.0",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-image-file-resizer": "^0.4.7",
    "react-infinite-scroll-component": "^6.0.0",
    "react-popper": "^2.2.5",
    "react-responsive-masonry": "^2.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-select": "^4.2.1",
    "sass": "^1.45.0",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:sass": "sass sass/main.scss public/stylesheets/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" public/stylesheets/style.comp.css -o src/style.css",
    "build:css": "npm-run-all compile:sass prefix:css",
    "watch:sass": "sass sass/main.scss src/style.css -w",
    "start:css": "npm-run-all --parallel watch:sass build:css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
